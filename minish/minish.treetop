grammar Minish

  rule element
    space* name (class / id / pseudo)* (space element)* space* block?
  end

  rule block
    space* '{' (![{}] element)+ '}'
  end

  rule class
    '.' name
  end

  rule id
    '#' name
  end

  rule pseudo
    ':' name
  end

  rule name
    [A-Za-z_]+
  end

  rule space
    [ \t\n\r]
  end

end
